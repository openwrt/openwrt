+static int php_sqlite_forget_persistent_id_numbers(zend_rsrc_list_entry *rsrc TSRMLS_DC)
+{
+	struct php_sqlite_db *db;
+
+	if (Z_TYPE_P(rsrc) != le_sqlite_pdb) {
+		return 0;
+	}
+
+	db = (struct php_sqlite_db*)rsrc->ptr;
