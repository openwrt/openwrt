/dts-v1/;

#include "rtl930x.dtsi"
#include "rtl93xx_linksys_lgs3xxc_nand_common.dtsi"

/ {
	compatible = "linksys,lgs328c", "realtek,rtl9301-soc";
	model = "Linksys LGS328C";
};

&i2c_mst1 {
	status = "okay";

	i2c2: i2c@2 {
		reg = <2>;
	};
	i2c3: i2c@3 {
		reg = <3>;
	};
	i2c4: i2c@4 {
		reg = <4>;
	};
	i2c5: i2c@5 {
		reg = <5>;
	};
};

&sfp0 {
	i2c-bus = <&i2c2>;
};

&sfp1 {
	i2c-bus = <&i2c3>;
};

&sfp2 {
	i2c-bus = <&i2c4>;
};

&sfp3 {
	i2c-bus = <&i2c5>;
};

&mdio_aux {
	status = "okay";
	gpio1: expander@0 {
		compatible = "realtek,rtl8231";
		reg = <0>;

		gpio-controller;
		#gpio-cells = <2>;
		gpio-ranges = <&gpio1 0 0 37>;

		led-controller {
			compatible = "realtek,rtl8231-leds";
			status = "disabled";
		};
	};
};

