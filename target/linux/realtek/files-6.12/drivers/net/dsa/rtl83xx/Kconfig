config NET_DSA_RTL83XX
	tristate "Realtek RTL838x/RTL839x switch support"
	depends on MACH_REALTEK_RTL
	select NET_DSA_TAG_TRAILER
	help
	  This driver adds support for Realtek RTL83xx series switching.

