. /lib/functions.sh

case "$(board_name)" in
	bananapi,bpi-r64)
	uci set system.@system[0].compat_version="1.2"
	uci commit system
	;;
	buffalo,wsr-2533dhp2)
	uci set system.@system[0].compat_version="1.1"
	uci commit system
	;;
	linksys,e8450-ubi)
	uci set system.@system[0].compat_version="2.0"
	uci commit system
	;;
esac

exit 0
