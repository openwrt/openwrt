+static int img_resume(struct ieee80211_hw *hw)
+{
+	int i = 0;
+	int active_vif_index = -1;
+	struct mac80211_dev *dev = NULL;
+
+	if (hw == NULL) {
+		pr_err("%s: Invalid parameters\n",
+		       __func__);
