+static inline int conv_str_to_byte(unsigned char *byte,
+		     unsigned char *str,
+		     int len)
+{
+	int  i, j = 0;
+	unsigned char ch, val = 0;
+
+	for (i = 0; i < (len * 2); i++) {
+		/*convert to lower*/
+		ch = ((str[i] >= 'A' && str[i] <= 'Z') ? str[i] + 32 : str[i]);
+
+		if ((ch < '0' || ch > '9') && (ch < 'a' || ch > 'f'))
+			return -1;
+
+		if (ch >= '0' && ch <= '9')  /*check is digit*/
+			ch = ch - '0';
+		else
+			ch = ch - 'a' + 10;
+
+		val += ch;
+
+		if (!(i%2))
+			val <<= 4;
+		else {
+			byte[j] = val;
+			j++;
+			val = 0;
+		}
+	}
+
+	return 0;
+}
