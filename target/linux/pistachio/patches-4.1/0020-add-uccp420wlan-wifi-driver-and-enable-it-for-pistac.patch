+
+	mutex_unlock(&dev->mutex);
+
+	if (!wait_for_econ_ps_cfg(dev)) {
+		if (!dev->econ_ps_cfg_stats.result) {
+			dev->power_save = PWRSAVE_STATE_AWAKE;
+			pr_debug("%s: Successful\n",
+				 __func__);
+
+			return 0;
