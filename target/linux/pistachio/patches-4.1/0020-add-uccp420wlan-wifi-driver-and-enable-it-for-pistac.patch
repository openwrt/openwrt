+static int chg_irq_register(int val)
+{
+	pr_debug("%s: change irq regist state %s.\n",
+		 hal_name, ((val == 1) ? "ON" : "OFF"));
+
+	if (val == 0) {
+		/* Unregister irq handler */
+		free_irq(hpriv->irq, hpriv);
+
+	} else if (val == 1) {
+		/* Register irq handler */
+		if (request_irq(hpriv->irq,
+				hal_irq_handler,
+				0,
+				"wlan",
+				hpriv) != 0) {
+			return -1;
+		}
+	}
+
+	return 0;
+}
