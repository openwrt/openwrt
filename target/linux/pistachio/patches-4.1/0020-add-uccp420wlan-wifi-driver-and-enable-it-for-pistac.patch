+
+	/* Not a BIP frame */
+	if (((hdr->frame_control & IEEE80211_FCTL_STYPE) ==
+	     IEEE80211_STYPE_DISASSOC) ||
+	    ((hdr->frame_control & IEEE80211_FCTL_STYPE) ==
+	     IEEE80211_STYPE_DEAUTH) ||
