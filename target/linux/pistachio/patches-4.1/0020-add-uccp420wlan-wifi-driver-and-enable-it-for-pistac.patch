+	if (WARN_ON((wifi->params.hw_scan_status == HW_SCAN_STATUS_PROGRESS)))
+		return -1;
+
+	dev = (struct mac80211_dev *)hw->priv;
+
+	mutex_lock(&dev->mutex);
+
+	for (i = 0; i < MAX_VIFS; i++) {
+		if (dev->active_vifs & (1 << i)) {
+			active_vif_index = i;
+			count++;
+		}
