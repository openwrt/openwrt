&edma {
	status = "okay";
};

&soc {
	dp6: dp6 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <6>;
		reg = <0x3a001800 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		phy-handle = <&qca8081_28>;
		phy-mode = "sgmii";
	};

	dp5: dp5 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <5>;
		reg = <0x3a001a00 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		phy-handle = <&qca8081_24>;
		phy-mode = "sgmii";
	};
};

&wifi {
	status = "okay";

	qcom,ath11k-calibration-variant = "Edgecore-EAP102";
};
