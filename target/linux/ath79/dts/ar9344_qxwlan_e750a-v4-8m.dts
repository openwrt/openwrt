#include "ar9344_qxwlan_e750x.dtsi"

/ {
	model = "Qxwlan E750A v4 8M";
	compatible = "qxwlan,e750a-v4-8m", "qca,ar9344";
};

&leds {
	lan {
		label = "green:lan";
		gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
	};

	wan {
		label = "green:wan";
		gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&swphy4>;

	nvmem-cells = <&macaddr_pridata_400 1>;
	nvmem-cell-names = "mac-address";
};

&eth1 {
	status = "okay";

	nvmem-cells = <&macaddr_pridata_400 0>;
	nvmem-cell-names = "mac-address";

	gmac-config {
		device = <&gmac>;
		switch-phy-swap = <0>;
		switch-only-mode = <1>;
	};
};

&partitions {
	partition@70000 {
		compatible = "denx,uimage";
		label = "firmware";
		reg = <0x070000 0x790000>;
	};
};
