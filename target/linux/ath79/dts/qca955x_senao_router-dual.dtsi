#include "qca955x.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};
};

&eth0 {
	status = "okay";

	pll-data = <0xa6000000 0x00000101 0x00001616>;

	nvmem-cells = <&macaddr_uboot_eth 0>;
	nvmem-cell-names = "mac-address";

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&mdio0 {
	status = "okay";

	ethernet-switch@0 {
		compatible = "qca,qca8327";
		#address-cells = <1>;
		#size-cells = <0>;

		reg = <0>;

		ethernet-ports {
			#address-cells = <1>;
			#size-cells = <0>;

			ethernet-port@1 {
				reg = <1>;
				label = "lan4";
				phy-mode = "internal";
				phy-handle = <&phy_port1>;
			};

			ethernet-port@2 {
				reg = <2>;
				label = "lan3";
				phy-mode = "internal";
				phy-handle = <&phy_port2>;
			};

			ethernet-port@3 {
				reg = <3>;
				label = "lan2";
				phy-mode = "internal";
				phy-handle = <&phy_port3>;
			};

			ethernet-port@4 {
				reg = <4>;
				label = "lan1";
				phy-mode = "internal";
				phy-handle = <&phy_port4>;
			};

			ethernet-port@5 {
				reg = <5>;
				label = "wan";
				phy-mode = "internal";
				phy-handle = <&phy_port5>;
			};

			ethernet-port@6 {
				reg = <6>;
				ethernet = <&eth0>;
				phy-mode = "rgmii-id";

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};

		mdio {
			#address-cells = <1>;
			#size-cells = <0>;

			phy_port1: ethernet-phy@0 {
				reg = <0>;
			};

			phy_port2: ethernet-phy@1 {
				reg = <1>;
			};

			phy_port3: ethernet-phy@2 {
				reg = <2>;
			};

			phy_port4: ethernet-phy@3 {
				reg = <3>;
			};

			phy_port5: ethernet-phy@4 {
				reg = <4>;
			};
		};
	};
};

&pcie0 {
	status = "okay";

	wifi0: wifi@0,0 {
		reg = <0x0000 0 0 0 0>;
	};
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x030000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x030000 0x010000>;
				read-only;

				nvmem-layout {
					compatible = "u-boot,env";
					#address-cells = <1>;
					#size-cells = <1>;

					macaddr_uboot_eth: ethaddr {
						#nvmem-cell-cells = <1>;
					};
				};
			};

			partition@40000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x040000 0xe50000>;
			};

			partition@e90000 {
				label = "manufacture";
				reg = <0xe90000 0x100000>;
				read-only;
			};

			partition@f90000 {
				label = "backup";
				reg = <0xf90000 0x010000>;
				read-only;
			};

			partition@fa0000 {
				label = "storage";
				reg = <0xfa0000 0x050000>;
				read-only;
			};

			partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;

				nvmem: nvmem-layout {
					compatible = "fixed-layout";
					#address-cells = <1>;
					#size-cells = <1>;

					calibration_art_1000: calibration@1000 {
						reg = <0x1000 0x440>;
					};
				};
			};
		};
	};
};

&wmac {
	status = "okay";
};
