#include "qca9563_ubnt_unifiac-pro.dtsi"

/ {
	compatible = "ubnt,unifiac-pro", "qca,qca9563";
	model = "Ubiquiti UniFi AC Pro";
};

&mdio0 {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	qca8337_port1: ethernet-phy@0 {
		reg = <0x0>;
	};

	qca8337: switch@10 {
		compatible = "qca,qca8337";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x10>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				ethernet = <&eth0>;
				phy-mode = "gmii";

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};

			port@1 {
				reg = <1>;
				label = "eth0";
				phy-mode = "internal";
				phy-handle = <&qca8337_port1>;
			};

			port@2 {
				reg = <2>;
				label = "eth1";
				ethernet = <&eth1>;
				phy-mode = "gmii";

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};
	};
};

&eth0 {
	status = "okay";

	pll-data = <0x03000101 0x00000101 0x00001919>;

	nvmem-cells = <&macaddr_art_0>;
	nvmem-cell-names = "mac-address";

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&usb_phy0 {
	status = "okay";
};

&usb0 {
	status = "okay";
};
