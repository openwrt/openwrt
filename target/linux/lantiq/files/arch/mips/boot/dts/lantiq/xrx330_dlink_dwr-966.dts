/dts-v1/;

#include "xrx330.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	compatible = "dlink,dwr-966", "lantiq,xway", "lantiq,grx390";
	model = "D-Link DWR-966";

	chosen {
		bootargs = "console=ttyLTQ0,115200";
	};

	memory@0 {
		reg = <0x0 0x8000000>;
	};

	aliases {
		led-boot = &led_power_error;
		led-failsafe = &led_power_error;
		led-running = &led_power;
		led-upgrade = &led_power_error;

		led-internet = &led_internet;
		led-wifi = &led_wifi_2g;
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		reset {
			label = "reset";
			gpios = <&gpio 9 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		ant {
			label = "ant";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_0>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led-0 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WPS;
			gpios = <&stp 23 GPIO_ACTIVE_LOW>;
		};

		led_internet: led-1 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WAN_ONLINE;
			gpios = <&stp 22 GPIO_ACTIVE_LOW>;
		};

		led-2 {
			color = <LED_COLOR_ID_GREEN>;
			function = "ant_ind";
			gpios = <&stp 20 GPIO_ACTIVE_LOW>;
		};

		led-3 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN_5GHZ;
			gpios = <&stp 14 GPIO_ACTIVE_LOW>;
		};

		led_wifi_2g: led-4 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN_2GHZ;
			gpios = <&stp 13 GPIO_ACTIVE_LOW>;
		};

		led-5 {
			color = <LED_COLOR_ID_GREEN>;
			function = "4g_link";
			gpios = <&stp 7 GPIO_ACTIVE_LOW>;
		};

		led-6 {
			color = <LED_COLOR_ID_GREEN>;
			function = "3g_link";
			gpios = <&stp 15 GPIO_ACTIVE_LOW>;
		};

		led-7 {
			color = <LED_COLOR_ID_BLUE>;
			function = "2g_link";
			gpios = <&stp 4 GPIO_ACTIVE_HIGH>;
		};

		led_power_error: led-8 {
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_POWER;
			gpios = <&stp 1 GPIO_ACTIVE_LOW>;
		};

		led_power: led-9 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_POWER;
			gpios = <&stp 0 GPIO_ACTIVE_LOW>;
		};

		led-10 {
			color = <LED_COLOR_ID_GREEN>;
			function = "rssi";
			function-enumerator = <1>;
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};

		led-11 {
			color = <LED_COLOR_ID_GREEN>;
			function = "rssi";
			function-enumerator = <2>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		led-12 {
			color = <LED_COLOR_ID_GREEN>;
			function = "rssi";
			function-enumerator = <3>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		led-13 {
			color = <LED_COLOR_ID_GREEN>;
			function = "rssi";
			function-enumerator = <4>;
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		led-14 {
			color = <LED_COLOR_ID_GREEN>;
			function = "rssi";
			function-enumerator = <5>;
			gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
		};
	};
};

&localbus {
	nand@1 {
		compatible = "lantiq,nand-xway";
		lantiq,cs = <1>;
		bank-width = <2>;
		reg = <1 0x0 0x2000000>;
		#address-cells = <1>;
		#size-cells = <1>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "uboot";
				reg = <0x0 0x100000>;
				read-only;
			};

			partition@100000 {
				label = "ubootconfigA";
				reg = <0x100000 0x40000>;
				read-only;
			};

			partition@140000 {
				label = "ubootconfigB";
				reg = <0x140000 0x40000>;
				read-only;
			};

			partition@180000 {
				label = "gphyfirmware";
				read-only;
				reg = <0x180000 0x40000>;
			};

			partition@1c0000 {
				label = "ubi";
				reg = <0x1c0000 0x7c00000>;
			};

			partition@7dc0000 {
				label = "calibration";
				reg = <0x7dc0000 0x100000>;
				read-only;
			};

			partition@7ec0000 {
				label = "factory";
				reg = <0x7ec0000 0x100000>;
				read-only;
			};

			partition@7fc0000 {
				label = "res";
				reg = <0x7fc0000 0x40000>;
				read-only;
			};
		};
	};
};

&gswip_mdio {
	phy1: ethernet-phy@1 {
		reg = <0x1>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@1 {
				reg = <1>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
			};
		};
	};

	phy2: ethernet-phy@2 {
		reg = <0x2>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@1 {
				reg = <1>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
			};

		};
	};

	phy3: ethernet-phy@3 {
		reg = <0x3>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@1 {
				reg = <1>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
			};

		};
	};

	phy4: ethernet-phy@4 {
		reg = <0x4>;

		leds {
			#address-cells = <1>;
			#size-cells = <0>;

			led@1 {
				reg = <1>;
				color = <LED_COLOR_ID_GREEN>;
				function = LED_FUNCTION_LAN;
			};
		};
	};
};

&gswip_ports {
	port@1 {
		reg = <1>;
		label = "lan3";
		phy-mode = "internal";
		phy-handle = <&phy1>;
	};

	port@2 {
		reg = <2>;
		label = "lan1";
		phy-mode = "internal";
		phy-handle = <&phy2>;
	};

	port@3 {
		reg = <3>;
		label = "lan4";
		phy-mode = "internal";
		phy-handle = <&phy3>;
	};

	port@4 {
		reg = <4>;
		label = "lan2";
		phy-mode = "internal";
		phy-handle = <&phy4>;
	};
};

&gpio {
	pinctrl-names = "default";
	pinctrl-0 = <&state_default>;

	state_default: pinmux {
		nand_out {
			lantiq,groups = "nand cle",
					"nand ale",
					"nand rd",
					"nand d1",
					"nand d0",
					"nand d2",
					"nand d7",
					"nand d6",
					"nand d5",
					"nand d4",
					"nand d3",
					"nand wr",
					"nand wp",
					"nand se";
			lantiq,function = "ebu";
			lantiq,output = <1>;
			lantiq,open-drain = <0>;
			lantiq,pull = <0>;
		};

		nand {
			lantiq,groups = "nand cs1", "nand rdy";
			lantiq,function = "ebu";
			lantiq,pull = <0>;
		};
	};
};

&stp {
	status = "okay";
	lantiq,phy1 = <0x2>;
	lantiq,phy2 = <0x2>;
	lantiq,phy3 = <0x2>;
	lantiq,phy4 = <0x2>;
};

&usb_phy0 {
	status = "okay";
};

&usb0 {
	status = "okay";
};

&usb_phy1 {
	status = "okay";
};

&usb1 {
	status = "okay";
};
