/dts-v1/;

#include "rtl839x.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	compatible = "netgear,gs728tpv2", "realtek,rtl838x-soc";
	model = "Netgear GS728TPv2";

	aliases {
		led-boot = &led_sys;
		led-failsafe = &led_sys;
		led-running = &led_sys;
		led-upgrade = &led_sys;
	};	

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		led_sys: sys {
			label = "gs728tpv2:green:sys";
			gpios = <&gpio0 47 GPIO_ACTIVE_LOW>;
		};
	};
};

&gpio0 {
	indirect-access-bus-id = <0>;
};

&spi0 {
	status = "okay";
	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x00000000 0xe0000>;
				read-only;
			};
			partition@e0000 {
				label = "u-boot-env";
				reg = <0x000e0000 0x10000>;
				read-only;
			};
			partition@f0000 {
				label = "u-boot-env2";
				reg = <0x000f0000 0x10000>;
				read-only;
			};
			partition@100000 {
				label = "jffs2_cfg";
				reg = <0x00100000 0x100000>;
			};
			partition@200000 {
				label = "jffs2_log";
				reg = <0x00200000 0x100000>;
			};
			partition@300000 {
				label = "rootfs";
				compatible = "denx,uimage";
				reg = <0x00300000 0xe80000>;
			};
			partition@1180000 {
				label = "firmware2";
				compatible = "netgear,uimage";
				reg = <0x01180000 0xe80000>;
			};
		};
	};
};

&ethernet0 {
