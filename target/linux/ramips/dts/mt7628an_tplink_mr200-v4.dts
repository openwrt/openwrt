/dts-v1/;

#include "mt7628an.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "tplink,mr200-v4", "mediatek,mt7628an-soc";
	model = "TP-Link Archer MR200 V4";

	aliases {
		led-boot = &led_power;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		lan {
			label = "white:lan";
			gpios = <&gpio 5 GPIO_ACTIVE_LOW>;
		};

		wan {
			label = "white:wan";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};

		led_power: power {
			label = "white:power";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
		};

		signal1 {
			label = "white:signal1";
			gpios = <&gpio 9 GPIO_ACTIVE_LOW>;
		};

		signal2 {
			label = "white:signal2";
			gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
		};

		signal3 {
			label = "white:signal3";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "white:wlan";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		rfkill {
			label = "rfkill";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RFKILL>;
		};
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x20000>;
				read-only;
			};

			partition@20000 {
				compatible = "tplink,firmware";
				label = "firmware";
				reg = <0x20000 0x7b0000>;
			};

			rom: partition@7d0000 {
				label = "rom";
				reg = <0x7d0000 0x10000>;
				read-only;
			};

			romfile: partition@7e0000 {
				label = "romfile";
				reg = <0x7e0000 0x10000>;
				read-only;
			};

			radio: partition@7f0000 {
				label = "radio";
				reg = <0x7f0000 0x10000>;
				read-only;
			};
		};
	};
};

&ehci {
	status = "okay";
};

&ohci {
	status = "okay";
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "refclk", "sdmode", "uart1", "uart2", "wdt", "p2led_an", "p1led_an", 
"p0led_an", 
"wled_an";
			ralink,function = "gpio";
		};
	};
};

&wmac {
	mediatek,mtd-eeprom = <&radio 0>;
	mtd-mac-address = <&romfile 0xf100>;
	status = "okay";
};

&ethernet {
	mtd-mac-address = <&romfile 0xf100>;
	/* mediatek,portmap = "llll"; */
	mediatek,portmap = <0x3f>;
};

&pcie {
	status = "okay";
};

&pcie0 {
	mt76@0,0 {
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&radio 0x8000>;
	};
};
