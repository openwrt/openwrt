	ucidef_set_led_netdev "lan" "lan" "green:lan" eth0.1
	ucidef_set_led_netdev "wan" "wan" "green:wan" eth0.2
	;;
asus,rt-n14u)
	ucidef_set_led_netdev "lan" "lan" "blue:lan" eth0.1
	ucidef_set_led_netdev "wan" "wan" "blue:wan" eth0.2
