trendnet,tew-810dr)
	ucidef_set_led_switch "wan" "wan" "green:wan" "switch0" "0x10"
	;;
dlink,dwr-118-a1)
	ucidef_set_led_switch "lan" "lan" "green:lan" "switch0" "0x1f"
	ucidef_set_led_switch "wan" "wan" "green:wan" "switch0" "0x20"
	;;
