			"0:lan" "1:lan" "2:lan" "3:lan" "4:wan" "7t@eth0"
		;;
	*)
		RT3X5X=`cat /proc/cpuinfo | egrep "(RT3.5|RT5350)"`
		if [ -n "${RT3X5X}" ]; then
			ramips_setup_rt3x5x_vlans
		else
			ucidef_set_interfaces_lan_wan "eth0.1" "eth0.2"
		fi
		;;
	esac
