. /lib/functions.sh
. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
	ubnt,edgerouter-x|\
	ubnt,edgerouter-x-sfp|\
	iptime,ax2002m|\
	iptime,ax2004m)
		ucidef_set_compat_version "2.0"
		;;
	tplink,er605-v2)
		ucidef_set_compat_version "1.2"
		;;
	*)
		ucidef_set_compat_version "1.1"
		;;
esac

board_config_flush

exit 0
