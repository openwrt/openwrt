	ucidef_set_led_netdev "lan2" "LAN2" "green:lan-2" "lan2"
	ucidef_set_led_netdev "lan3" "LAN3" "green:lan-3" "lan3"
	ucidef_set_led_netdev "lan4" "LAN4" "green:lan-4" "lan4"
	ucidef_set_led_netdev "wan" "WAN" "green:wan" "wan"
