	ucidef_set_led_netdev "wan" "wan link" "blue:wan" "wan" "link"
	;;
linksys,e7350)
	ucidef_set_led_netdev "lan1" "lan1" "blue:lan-1" "lan1"
	ucidef_set_led_netdev "lan2" "lan2" "blue:lan-2" "lan2"
