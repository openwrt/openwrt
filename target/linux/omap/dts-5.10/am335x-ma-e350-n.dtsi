		pinctrl-single,pins = <
			AM33XX_PADCONF(AM335X_PIN_GPMC_AD15,  PIN_OUTPUT, MUX_MODE7) /* gpmc_ad15.gpio1_15 */
			AM33XX_PADCONF(AM335X_PIN_LCD_DATA8,  PIN_OUTPUT, MUX_MODE7) /* lcd_data8.gpio2_14 */
