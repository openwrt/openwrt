-			       INT_IDX0_MASK);
-	airoha_qdma_irq_enable(&qdma->irq_banks[0], QDMA_INT_REG_IDX1,
-			       INT_IDX1_MASK);
+			       TX_COHERENT_LOW_INT_MASK | INT_TX_MASK);
