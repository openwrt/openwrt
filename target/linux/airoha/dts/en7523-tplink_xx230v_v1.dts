/dts-v1/;

#include <dt-bindings/leds/common.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "en7523.dtsi"

/ {
	model = "TP-Link xx230v/xx530v v1";
	compatible = "tplink,xx230v,v1", "airoha,en7523";

	chosen {
		bootargs = "console=ttyS0,115200 earlycon";
		stdout-path = "serial0:115200n8";
	};

	aliases {
		serial0 = &uart1;
		led-boot = &led_power;
		led-failsafe = &led_power;
		led-upgrade = &led_power;
		led-running = &led_power;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x40000000>;
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&pinctrl 0 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&pinctrl 7 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		// Power
		led_power: led@0 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <0>;
			gpios = <&pinctrl 11 GPIO_ACTIVE_LOW>;
		};

		// GPON
		led@1 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <1>;
			gpios = <&pinctrl 10 GPIO_ACTIVE_LOW>;
		};

		// internet
		led@3 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <3>;
			gpios = <&pinctrl 1 GPIO_ACTIVE_LOW>;
		};

		// Wireless
		led@4 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN;
			function-enumerator = <0>;
			gpios = <&pinctrl 6 GPIO_ACTIVE_LOW>;
		};

		// Phone
		led@5 {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <4>;
			gpios = <&pinctrl 5 GPIO_ACTIVE_HIGH>;
		};

		// LOS
		led@2 {
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_STATUS;
			function-enumerator = <0>;
			gpios = <&pinctrl 3 GPIO_ACTIVE_HIGH>;
		};
	};
};

&pinctrl {
	gpio-ranges = <&pinctrl 0 0 32>;
};

&nand {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "bootloader";
			reg = <0x00000 0x80000>;
			read-only;
		};

		partition@80000 {
			label = "romfile";
			reg = <0x80000 0xa0000>;
			read-only;
		};

		partition@a0000 {
			label = "misc";
			reg = <0x0a0000 0x1a0000>;
			read-only;

			nvmem-layout {
				compatible = "fixed-layout";
				#address-cells = <1>;
				#size-cells = <1>;

				macaddr: macaddr@4f100 {
					#nvmem-cell-cells = <1>;
					compatible = "mac-base";
					reg = <0x4f100 0x6>;
				};

				eeprom0: eeprom@80000 {
					reg = <0x80000 0x1000>;
				};

				en7570_bob: en7570_bob@c0000 {
					reg = <0xc0000 0xff>;
				};
			};
		};

		partition@1a0000 {
			label = "kernel_dummy";
			reg = <0x01a0000 0x0>;
			read-only;
		};

		// Fist 512 have header of kernel
		// flash write 0x01a0000 0x$filesize $loadaddr
		//
		// To flash rootfs
		// flash write 0x11a0000 0x$filesize $loadaddr
		partition@01a0200 {
			label = "kernel";
			reg = <0x01a0000 0x1000000>;
		};

		partition@11a0000 {
			label = "rootfs";
			reg = <0x11a0000 0x6360000>;
		};

		/* reserved for bad block table */
		reserved_bmt@3fe0000 {
			label = "reserved_bmt";
			reg = <0x7500000 0x0b00000>;
			// reg = <0x3fe0000 0x200000>;
			read-only;
		};
	};
};

&pcie0 {
	status = "okay";
};

&pcie1 {
	status = "okay";

	pcie@1,0 {
		reg = <0x00000800 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;

		mt7915@0,0 {
			reg = <0x0000 0 0 0 0>;
			#address-cells = <1>;
			#size-cells = <0>;
			nvmem-cells = <&eeprom0>;
			nvmem-cell-names = "eeprom";
			airoha,npu = <&npu>;
			airoha,eth = <&eth>;

			// 2.4 GHz
			band@0 {
				reg = <0>;
				nvmem-cells = <&macaddr 0>;
				nvmem-cell-names = "mac-address";
			};

			// 5 GHz
			band@1 {
				reg = <1>;
				nvmem-cells = <&macaddr 2>;
				nvmem-cell-names = "mac-address";
			};
		};
	};
};

&usb0 {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&xpon_phy {
	status = "okay";

	nvmem-cells = <&en7570_bob>;
	nvmem-cell-names = "phy-bob";
};

&xpon {
	status = "okay";
};

&npu {
	status = "okay";
};

&eth {
	status = "okay";
};

&gdm1 {
	status = "okay";

	nvmem-cells = <&macaddr 0>;
	nvmem-cell-names = "mac-address";
};

&switch {
	status = "okay";
	ports {
		port@1 {
			label = "lan1";
			status = "okay";
		};

		port@2 {
			label = "lan2";
			status = "okay";
		};

		port@3 {
			label = "lan3";
			status = "okay";
		};

		port@4 {
			label = "wan";
			status = "okay";
		};

		port@6 {
			nvmem-cells = <&macaddr 0>;
			nvmem-cell-names = "mac-address";
		};
	};

	mdio {
		ethernet-phy@9 {
			leds {
				led0@0 {
					status = "okay";
				};
			};
		};

		ethernet-phy@10 {
			leds {
				led0@0 {
					status = "okay";
				};
			};
		};

		ethernet-phy@11 {
			leds {
				led0@0 {
					status = "okay";
				};
			};
		};

		ethernet-phy@12 {
			leds {
				led0@0 {
					status = "okay";
					function = LED_FUNCTION_WAN;
				};
			};
		};
	};
};
