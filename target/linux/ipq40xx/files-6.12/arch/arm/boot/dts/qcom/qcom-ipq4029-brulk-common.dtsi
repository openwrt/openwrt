	nvmem-cells = <&macaddr_wan>;
};

&swport2 {
	status = "okay";
	label = "lan4";
	nvmem-cell-names = "mac-address";
	nvmem-cells = <&macaddr_lan>;
};

&swport3 {
	status = "okay";
	label = "lan3";
	nvmem-cell-names = "mac-address";
	nvmem-cells = <&macaddr_lan>;
};

&swport4 {
	status = "okay";
	label = "lan2";
	nvmem-cell-names = "mac-address";
	nvmem-cells = <&macaddr_lan>;
