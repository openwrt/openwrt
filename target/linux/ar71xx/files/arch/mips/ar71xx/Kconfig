config SOC_AR71XX
	bool
	select USB_ARCH_HAS_EHCI
	select USB_ARCH_HAS_OHCI

config SOC_AR724X
	bool
	select USB_ARCH_HAS_EHCI
	select USB_ARCH_HAS_OHCI

config SOC_AR913X
	bool
	select USB_ARCH_HAS_EHCI

config SOC_AR934X
	bool
	select USB_ARCH_HAS_EHCI

config AR71XX_DEV_M25P80
	def_bool n

config AR71XX_DEV_AP91_PCI
	select AR71XX_PCI_ATH9K_FIXUP
	def_bool n

config AR71XX_DEV_AP94_PCI
	select AR71XX_PCI_ATH9K_FIXUP
	def_bool n

config AR71XX_DEV_AR9XXX_WMAC
	def_bool n

