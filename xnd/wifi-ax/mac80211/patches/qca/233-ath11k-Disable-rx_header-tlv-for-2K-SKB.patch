@@ -1030,6 +1098,14 @@ struct htt_rx_ring_tlv_filter {
 	u32 pkt_filter_flags1; /* MGMT */
 	u32 pkt_filter_flags2; /* CTRL */
 	u32 pkt_filter_flags3; /* DATA */
+	bool offset_valid;
+	u16 rx_packet_offset;
+	u16 rx_header_offset;
