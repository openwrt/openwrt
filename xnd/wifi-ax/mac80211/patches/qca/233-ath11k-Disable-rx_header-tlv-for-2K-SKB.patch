@@ -1022,6 +1086,10 @@ struct htt_rx_ring_selection_cfg_cmd {
 	u32 pkt_type_en_flags2;
 	u32 pkt_type_en_flags3;
 	u32 rx_filter_tlv;
+	u32 rx_packet_offset;
+	u32 rx_mpdu_offset;
+	u32 rx_msdu_offset;
+	u32 rx_attn_offset;
 } __packed;
 
