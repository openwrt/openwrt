+		goto free_out;
+	}
+
 	rxcb = ATH11K_SKB_RXCB(msdu);
 	rxcb->rx_desc = rx_desc;
 	msdu_len = ath11k_dp_rx_h_msdu_start_msdu_len(ab, rx_desc);
@@ -2683,8 +2804,9 @@ static int ath11k_dp_rx_process_msdu(str
 			hdr_status = ath11k_dp_rx_h_80211_hdr(ab, rx_desc);
