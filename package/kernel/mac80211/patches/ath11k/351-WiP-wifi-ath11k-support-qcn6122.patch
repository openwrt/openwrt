 	if (ret) {
 		ath11k_err(ab, "failed to get rproc: %d\n", ret);
 		goto err_ce_free;
 	}
 
+	ath11k_ahb_init_qmi_ce_config(ab);
+
 	ret = ath11k_core_init(ab);
 	if (ret) {
