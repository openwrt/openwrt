+	tlv_filter.offset_valid = false;
 
 	for (i = 0; i < ab->hw_params.num_rxmda_per_pdev; i++) {
 		ring_id = ar->dp.rx_mon_status_refill_ring[i].refill_buf_ring.ring_id;
@@ -1111,6 +1112,7 @@ static ssize_t ath11k_write_pktlog_filte
 	}
 
