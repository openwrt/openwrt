+U_BOOT_DRIVER(airoha_pcs) = {
+	.name = "airoha-pcs",
+	.id = UCLASS_MISC,
+	.of_match = airoha_pcs_of_table,
+	.probe = airoha_pcs_probe,
+	.priv_auto = sizeof(struct airoha_pcs_priv),
+};
