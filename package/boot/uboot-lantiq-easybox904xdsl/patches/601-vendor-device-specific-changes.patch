+
+    if((retVal = rtl8367b_setAsicReg(0x1370, 0x0002)) != RT_ERR_OK)
+        return retVal;
+
+    polling_time = 0;
+    while(polling_time < 10000)
+    {
+        if((retVal = rtl8367b_getAsicReg(0x1370, &data)) != RT_ERR_OK)
+            return retVal;
+
+        if((data & 0x0004) == 0)
