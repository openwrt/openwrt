+		lcd_WinBr_y = x + width - 1;
+	}
+	else
+	{
+		lcd_WinTl_x = x;
+		lcd_WinTl_y = y;
+		lcd_WinBr_x = x + width - 1;
+		lcd_WinBr_y = y + height - 1;
