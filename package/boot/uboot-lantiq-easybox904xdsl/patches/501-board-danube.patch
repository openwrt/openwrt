+	li  t2, 0x80000000
+	sw	t2,0(t1)
+
+	li  t1, PCI_CR_PCI_MOD_REG	//mw be105430 103
+	li  t2, 0x103
+	sw  t2,0(t1)
+	
+	li  t1, CS_CFM			//mw b700006c 0 
+	li  t2, 0x00
+	sw  t2, 0(t1)		
+	
+	li  t1, PCI_CR_PCI_MOD_REG	//mw be105430 103
+	li  t2, 0x1000103
+	sw  t2, 0(t1)	
+#endif 
+//06063001-joelin disable the PCI CFRAME mask-end
+
+
+#ifdef  CONFIG_BOOT_FROM_NOR 	
+#ifdef CONFIG_TUNE_DDR
+  li      t1, 0xbf800060
+	lw      t2, 0(t1)
+	andi    t2, t2, 0x1
+	bne     t2, zero, ddr_init_finish
+	nop
+#endif
+  bal	ddrram_init
+	nop
+#endif
+ddr_init_finish:
+	move	ra, t0
+	j	ra
+	nop
+
+	.end	lowlevel_init
