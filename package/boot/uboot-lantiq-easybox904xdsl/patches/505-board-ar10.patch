+int ssc_write(unsigned char uc_value)
+{
+    ssc_sync();
+	*SPI_TB = (u32)uc_value;
+	ssc_sync();
+	return 0;
+}
+
+u8	ssc_read(void)
+{
+    ssc_sync();
+    *SPI_RXREQ = 1;
+    /* poll RIR */
+    while((SPI_FSTAT_RXFFL(*SPI_FSTAT)) == 0) { };
+	return (u8)(*SPI_RB);
+	
+}
