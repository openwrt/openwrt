			"items": {
				"type": "string"
			}
		},
		"network_auth_type": {
			"description": "Network Authentication Type",
			"type": "string"
		},
		"ocv": {
			"description": "Operating Channel Validation",
			"type": "number",
			"minimum": 0,
			"maximum": 15
		},
		"okc": {
			"description": "PMKSA and Opportunistic Key Caching",
