
config system
	option hostname 'LAKSAMANA'
	option timezone '<+08>-8'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option cronloglevel '9'
	option zonename 'Asia/Kuala Lumpur'
	option log_proto 'udp'
	option conloglevel '8'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config led 'led_5g'
	option name '5G'
	option sysfs 'blue:5g'
	option trigger 'netdev'
	option dev 'wwan0'
	list mode 'link'

config led 'led_wan'
	option name 'LAN'
	option sysfs 'green:internet'
	option trigger 'netdev'
	option dev 'br-lan'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led 'led_wan_port_link'
	option name 'WAN-PORT-LINK'
	option sysfs '90000.mdio-1:1c:green:wan'
	option trigger 'netdev'
	option mode 'tx rx link_10 link_100 link_1000 link_2500'
	option dev 'wan'

config imm_init
	option ipv6 '2'

config led
	option name '5G Down'
	option sysfs 'red:5g'
	option trigger 'netdev'
	option dev 'wwan0'

config led
	option name 'Signal'
	option sysfs 'blue:signal'
	option trigger 'netdev'
	option dev 'wwan0'
	list mode 'link'

config led
	option name 'Signal Down'
	option sysfs 'red:signal'
	option trigger 'netdev'
	option dev 'wwan0'

config led
	option name 'Power'
	option sysfs 'green:power'
	option trigger 'default-on'

config led
	option name 'Wi-Fi'
	option sysfs 'green:wifi'
	option trigger 'netdev'
	option dev 'phy0-ap0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'Wi-Fi'
	option sysfs 'green:wifi'
	option trigger 'netdev'
	option dev 'phy1-ap0'
	list mode 'link'
	list mode 'tx'

config led
	option name 'Phone'
	option sysfs 'green:phone'
	option trigger 'default-on'
