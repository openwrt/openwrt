diff -ruN a/libc/sysdeps/linux/arm/setcontext.S b/libc/sysdeps/linux/arm/setcontext.S
--- a/libc/sysdeps/linux/arm/setcontext.S	1970-01-01 08:00:00.000000000 +0800
+++ b/libc/sysdeps/linux/arm/setcontext.S	2015-07-06 04:09:46.000000000 +0800
@@ -0,0 +1,76 @@
